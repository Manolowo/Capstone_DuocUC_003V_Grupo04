                    "ven_descuento": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,10}(?:\\.\\d{0,2})?$",
                        "nullable": true
                    },
                    "ven_subtotal": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,10}(?:\\.\\d{0,2})?$",
                        "nullable": true
                    },
                    "bol": {
                        "type": "integer",
                        "nullable": true
                    },
                    "prod": {
                        "type": "integer",
                        "nullable": true
                    }
                },
                "required": [
                    "ven_id"
                ]
            }
        },
        "securitySchemes": {
            "jwtAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }

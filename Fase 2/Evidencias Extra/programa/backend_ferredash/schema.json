            "Categoria": {
                "type": "object",
                "properties": {
                    "cat_id": {
                        "type": "integer",
                        "readOnly": true
                    },
                    "cat_nom": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 100
                    }
                },
                "required": [
                    "cat_id"
                ]
            },
            "Cliente": {
                "type": "object",

                "type": "object",
                "properties": {
                    "prod_id": {
                        "type": "integer",
                        "readOnly": true
                    },
                    "prod_codigobarra": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 50
                    },
                    "prod_nom": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 100
                    },
                    "prod_desc": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 255
                    },
                    "prod_prec_compra_unitario": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,10}(?:\\.\\d{0,2})?$",
                        "nullable": true
                    },
                    "prod_prec_venta_neto": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,10}(?:\\.\\d{0,2})?$",
                        "nullable": true
                    },
                    "prod_prec_venta_final": {
                        "type": "string",
                        "format": "decimal",
                        "pattern": "^-?\\d{0,10}(?:\\.\\d{0,2})?$",
                        "nullable": true
                    },
                    "prod_afecto_iva": {
                        "type": "boolean",
                        "nullable": true
                    },
                    "prod_tipo_unidad": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 50
                    },
                    "prod_marca": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 50
                    },
                    "prod_talla": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 20
                    },
                    "prod_color": {
                        "type": "string",
                        "nullable": true,
                        "maxLength": 30
                    },
                    "cat": {
                        "type": "integer",
                        "nullable": true
                    }
                },
                "required": [
                    "prod_id"
                ]
            },
